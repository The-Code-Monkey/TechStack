{
  "version": "0.2.57",
  "license": "MIT",
  "bin": {
    "orchard": "./dist/esm/index.js"
  },
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=16"
  },
  "scripts": {
    "build": "tsc -p tsconfig.json && yarn copy",
    "copy": "cp ./src/config.json ./dist/esm/config.json && cp -r ./src/templates ./dist/esm && cp -r ./src/theme ./dist/esm && cp ./orchard.theme.config.json ./dist/esm",
    "lint": "tcm lint",
    "test": "yarn generate && yarn generate -b partial",
    "prepare": "yarn build",
    "size": "size-limit",
    "generate": "ts-node ./src/index.ts generate",
    "release": "yarn build && release-it --ci --npm.access=public"
  },
  "name": "@techstack/theme",
  "description": "A practical theme generator built from the ground up",
  "author": "The-Code-Monkey",
  "homepage": "https://github.com/The-Code-Monkey/TechStack/blob/dev/packages/theme/README.md",
  "repository": {
    "type": "git",
    "url": "https://github.com/The-Code-Monkey/TechStack.git"
  },
  "bugs": {
    "url": "https://github.com/The-Code-Monkey/TechStack/issues"
  },
  "publishConfig": {
    "access": "public"
  },
  "size-limit": [
    {
      "path": "dist/esm/index.js",
      "limit": "2 KB"
    }
  ],
  "devDependencies": {
    "@size-limit/file": "8.1.0",
    "@techstack/tcm-cli": "0.2.56",
    "@tsconfig/recommended": "1.0.1",
    "@types/fs-extra": "9.0.13",
    "@types/lodash": "4.14.186",
    "@types/lodash-es": "4.17.6",
    "@types/tinycolor2": "1.4.3",
    "size-limit": "8.1.0",
    "ts-jest": "29.0.3",
    "tslib": "2.4.0",
    "typescript": "4.8.4"
  },
  "dependencies": {
    "fs-extra": "10.1.0",
    "lodash": "4.17.21",
    "lodash-es": "4.17.21",
    "sade": "1.8.1",
    "style-dictionary": "3.7.1",
    "tinycolor2": "1.4.2"
  },
  "gitHead": "a0cda00211e6f1c53b567b16603afaf07f995eae"
}
