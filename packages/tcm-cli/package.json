{
  "name": "@aw-web-design/tcm-cli",
  "version": "2.0.2",
  "description": "Zero-config TypeScript package development",
  "license": "MIT",
  "repository": {
    "type": "git"
  },
  "keywords": [
    "react",
    "typescript",
    "bundle",
    "rollup"
  ],
  "bin": {
    "tcm": "./dist/index.js"
  },
  "typings": "./dist/index.d.ts",
  "scripts": {
    "prepare": "tsc -p tsconfig.json",
    "build": "tsc -p tsconfig.json",
    "lint": "yarn build && yarn lint:post-build",
    "lint:post-build": "node dist/index.js lint ./",
    "start": "tsc -p tsconfig.json --watch",
    "deduplicate": "yarn dedupe",
    "deduplicate:check": "yarn dedupe -c",
    "doctoc": "doctoc README.md"
  },
  "files": [
    "dist",
    "templates",
    "conf"
  ],
  "engines": {
    "node": ">=14.0.0"
  },
  "dependencies": {
    "@babel/core": "7.17.8",
    "@babel/helper-module-imports": "7.16.7",
    "@babel/parser": "7.17.8",
    "@babel/plugin-proposal-class-properties": "7.16.7",
    "@babel/preset-env": "7.16.11",
    "@babel/traverse": "7.17.3",
    "@rollup/plugin-babel": "5.3.1",
    "@rollup/plugin-commonjs": "21.0.3",
    "@rollup/plugin-json": "4.1.0",
    "@rollup/plugin-node-resolve": "13.1.3",
    "@rollup/plugin-replace": "4.0.0",
    "@types/jest": "27.4.1",
    "@typescript-eslint/eslint-plugin": "5.17.0",
    "@typescript-eslint/parser": "5.17.0",
    "ansi-escapes": "4.3.2",
    "asyncro": "3.0.0",
    "babel-plugin-annotate-pure-calls": "0.4.0",
    "babel-plugin-dev-expression": "0.2.3",
    "babel-plugin-macros": "3.1.0",
    "babel-plugin-polyfill-regenerator": "0.3.1",
    "babel-plugin-transform-rename-import": "2.3.0",
    "camelcase": "6.3.0",
    "chalk": "4.1.2",
    "confusing-browser-globals": "1.0.11",
    "enquirer": "2.3.6",
    "eslint": "8.12.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-flowtype": "8.0.3",
    "eslint-plugin-import": "2.25.4",
    "eslint-plugin-jest": "26.1.3",
    "eslint-plugin-jsx-a11y": "6.5.1",
    "eslint-plugin-prettier": "4.0.0",
    "eslint-plugin-react": "7.29.4",
    "eslint-plugin-react-hooks": "4.4.0",
    "eslint-plugin-testing-library": "5.2.0",
    "execa": "4.1.0",
    "figlet": "1.5.2",
    "fs-extra": "10.0.1",
    "jest": "27.5.1",
    "jest-watch-typeahead": "1.0.0",
    "jpjs": "1.2.1",
    "lodash.merge": "4.6.2",
    "ora": "5.4.1",
    "pascal-case": "3.1.2",
    "postcss": "8.4.12",
    "prettier": "2.6.2",
    "progress-estimator": "0.3.0",
    "regenerator-runtime": "0.13.9",
    "rollup": "2.70.1",
    "rollup-plugin-delete": "2.0.0",
    "rollup-plugin-dts": "4.2.0",
    "rollup-plugin-sourcemaps": "0.6.3",
    "rollup-plugin-terser": "7.0.2",
    "rollup-plugin-typescript2": "0.31.2",
    "sade": "1.8.1",
    "semver": "7.3.5",
    "shelljs": "0.8.5",
    "sort-package-json": "1.55.0",
    "tiny-glob": "0.2.9",
    "ts-jest": "27.1.4",
    "ts-node": "10.7.0",
    "tslib": "2.3.1",
    "type-fest": "2.12.2",
    "typescript": "4.6.3"
  },
  "devDependencies": {
    "@commitlint/cli": "16.2.3",
    "@commitlint/config-conventional": "16.2.1",
    "@release-it/conventional-changelog": "4.2.2",
    "@types/cssnano": "5.0.0",
    "@types/eslint": "8.4.1",
    "@types/figlet": "1.5.4",
    "@types/fs-extra": "9.0.13",
    "@types/lodash": "4.14.181",
    "@types/node": "16.11.26",
    "@types/react": "17.0.43",
    "@types/rollup-plugin-json": "3.0.3",
    "@types/semver": "7.3.9",
    "@types/shelljs": "0.8.11",
    "@types/styled-components": "5.1.24",
    "@typescript-eslint/eslint-plugin": "5.17.0",
    "@typescript-eslint/parser": "5.17.0",
    "auto-changelog": "2.4.0",
    "autoprefixer": "10.4.4",
    "babel-plugin-replace-identifiers": "0.1.1",
    "cssnano": "5.1.7",
    "doctoc": "2.1.0",
    "lint-staged": "12.3.7",
    "react": "18.0.0",
    "react-dom": "18.0.0",
    "react-is": "18.0.0",
    "release-it": "14.14.0",
    "rollup-plugin-postcss": "4.0.2",
    "styled-components": "5.3.5",
    "tiny-invariant": "1.2.0",
    "tiny-warning": "1.0.3"
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "release-it": {
    "git": {
      "commitMessage": "chore: release v${version}"
    },
    "plugins": {
      "@release-it/conventional-changelog": {
        "preset": "conventionalcommits",
        "infile": "CHANGELOG.md"
      }
    },
    "github": {
      "release": true
    },
    "npm": {
      "skipChecks": true
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,md,json,yml,yaml}": [
      "prettier -w"
    ],
    "*.{js,jsx,ts,tsx}": [
      "node dist/index.js lint --fix"
    ]
  }
}
