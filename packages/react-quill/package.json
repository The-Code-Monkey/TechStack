{
  "name": "@techstack/react-quill",
  "version": "0.0.2",
  "description": "The Quill rich-text editor as a React component.",
  "license": "MIT",
  "homepage": "https://github.com/The-Code-Monkey/TechStack/blob/dev/packages/react-quill/README.md",
  "bugs": "https://github.com/The-Code-Monkey/TechStack/issues",
  "repository": "https://github.com/The-Code-Monkey/TechStack.git",
  "main": "./dist/index.js",
  "module": "./dist/react-quill.esm.js",
  "types": "./dist/index.d.ts",
  "type": "module",
  "publishConfig": {
    "access": "public"
  },
  "typings": "dist/types/index.d.ts",
  "scripts": {
    "build": "tcm build && yarn build:css",
    "build:lib": "tsc",
    "build:dist": "webpack",
    "build:css": "cpx '../../node_modules/quill/dist/quill.*.css' dist",
    "watch": "tsc --watch",
    "lint": "tcm lint",
    "demo": "superstatic demo"
  },
  "keywords": [
    "react",
    "react-component",
    "rich",
    "text",
    "rich-text",
    "textarea",
    "quill"
  ],
  "files": [
    "dist/",
    "lib/",
    "types.d.ts",
    "README.md",
    "CHANGELOG.md",
    "LICENSE"
  ],
  "dependencies": {
    "@types/quill": "2.0.10",
    "lodash": "4.17.21",
    "quill": "2.0.0-dev.4",
    "quill-better-table": "^1.2.10"
  },
  "peerDependencies": {
    "react": "^16 || ^17 || ^18",
    "react-dom": "^16 || ^17 || ^18"
  },
  "devDependencies": {
    "@nrwl/nx-cloud": "16.0.1",
    "@techstack/tcm-cli": "0.4.110",
    "@types/lodash": "^4.14.146",
    "@types/react": "^18.0.38",
    "@types/react-dom": "^18.0.11",
    "blanket": "^1.2.3",
    "cheerio": "^1.0.0-rc.3",
    "cpx": "^1.5.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "should": "^13.2.3",
    "superstatic": "^9.0.3",
    "typescript": "^5.0.4",
    "webpack": "^5.80.0",
    "webpack-cli": "^5.0.2"
  }
}
