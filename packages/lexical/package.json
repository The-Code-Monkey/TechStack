{
  "name": "@techstack/lexical",
  "author": "The-Code-Monkey",
  "packageManager": "yarn@4.1.0",
  "description": "a wrapper for facebook/lexical for react",
  "version": "1.4.6",
  "scripts": {
    "build": "tcm build && cp ./src/index.scss ./dist/index.scss",
    "lint": "tcm lint",
    "test": "tcm test"
  },
  "license": "MIT",
  "dependencies": {
    "@lexical/react": "0.13.1",
    "@techstack/react-feather": "workspace:*",
    "lexical": "0.13.1",
    "react": "18.2.0",
    "styled-components": "6.1.1"
  },
  "devDependencies": {
    "@techstack/tcm-cli": "workspace:*"
  },
  "main": "./dist/index.js",
  "module": "./dist/lexical.esm.js",
  "exports": {
    ".": {
      "import": "./dist/lexical.esm.js",
      "require": "./dist/index.js"
    },
    "./css": {
      "import": "./dist/index.scss",
      "require": "./dist/index.scss"
    }
  },
  "typings": "dist/types/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=18"
  },
  "publishConfig": {
    "access": "public"
  }
}
