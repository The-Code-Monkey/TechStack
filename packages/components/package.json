{
  "version": "3.4.0",
  "license": "MIT",
  "type": "module",
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "exports": {
    "import": "./dist/esm/index.js",
    "require": "./dist/cjs/index.js",
    "default": "./dist/esm/index.js"
  },
  "typings": "dist/types/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=14"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "start": "tcm watch",
    "build": "yarn build:tokens && tcm build ",
    "build:tokens": "orchard generate",
    "test": "yarn build:tokens && tcm test",
    "lint": "yarn build:tokens && tcm lint",
    "size": "size-limit",
    "analyze": "size-limit --why",
    "storybook": "yarn build:tokens && start-storybook -p 6006",
    "build-storybook": "yarn build:tokens && build-storybook",
    "release": "yarn lint && yarn build && release-it --ci --npm.access=public"
  },
  "name": "@aw-web-design/components",
  "author": "The-Code-Monkey",
  "size-limit": [
    {
      "path": "dist/esm/index.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/cjs/index.js",
      "limit": "10 KB"
    }
  ],
  "jest": {
    "setupFiles": [
      "./setup.ts"
    ]
  },
  "devDependencies": {
    "@size-limit/file": "7.0.8",
    "@storybook/addon-essentials": "6.5.0-beta.5",
    "@storybook/addon-links": "6.5.0-beta.5",
    "@storybook/addons": "6.5.0-beta.5",
    "@storybook/react": "6.5.0-beta.5",
    "@testing-library/react": "13.2.0",
    "@tsconfig/create-react-app": "1.0.2",
    "@tsconfig/recommended": "1.0.1",
    "@types/react": "18.0.8",
    "@types/react-dom": "18.0.3",
    "@types/react-table": "7.7.11",
    "cross-env": "7.0.3",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-prettier": "4.0.0",
    "react-is": "18.1.0",
    "size-limit": "7.0.8",
    "tslib": "2.4.0",
    "typescript": "4.6.4",
    "esm": "3.2.25"
  },
  "dependencies": {
    "@aw-web-design/memoize": "1.0.3",
    "@aw-web-design/react-feather": "3.2.1",
    "@aw-web-design/styled-system": "3.3.1",
    "@aw-web-design/tcm-cli": "3.2.8",
    "@aw-web-design/theme": "3.2.1",
    "classnames": "2.3.1",
    "csstype": "3.0.11",
    "react": "18.1.0",
