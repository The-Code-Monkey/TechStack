  "scripts": {
    "start": "tcm watch",
    "build": "bun run build:tokens && tcm build",
    "build:tokens": "orchard generate",
    "test": "bun run build:tokens && bun test",
    "lint": "bun run build:tokens && tcm lint",
    "size": "size-limit",
    "analyze": "size-limit --why",
    "storybook": "bun run build:tokens && storybook dev -p 6006",
    "build-storybook": "bun run build:tokens && storybook build",
    "release": "bun run lint && bun run build && release-it --ci --npm.access=public",
    "chromatic": "npx chromatic"
  },
  "name": "@techstack/components",
