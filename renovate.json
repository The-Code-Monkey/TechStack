{
  "extends": [
    ":dependencyDashboard",
    ":semanticPrefixFixDepsChoreOthers",
    ":ignoreModulesAndTests",
    ":autodetectPinVersions",
    ":prConcurrentLimit20",
    "group:monorepos",
    "group:recommended",
    "workarounds:all"
  ],
  "rangeStrategy": "pin",
  "rollbackPrs": false,
  "labels": ["dependencies"],
  "automerge": true,
  "vulnerabilityAlerts": {
    "labels": ["security"]
  },
  "updateInternalDeps": true,
  "packageRules": [
    {
      "matchUpdateTypes": ["minor", "patch", "pin", "digest"],
      "automerge": true
    }
  ],
  "ignoreDeps": ["execa", "ora"]
}
